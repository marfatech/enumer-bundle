        "symfony/http-kernel":          "~3.4||~4.0||~5.0",
        "wakeapp/enumer":               "^1.0"
    },

    "autoload": {
        "psr-4": {
            "Wakeapp\\Bundle\\EnumerBundle\\": ""
        }
    }
